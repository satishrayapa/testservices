{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "Aumentum": "Data Source=#DATASOURCE;Database=#DATABASE;User Id=#USERID;Password=#PASSWORD;Connection Timeout=#DBTIMEOUT;"
  },
  "VirtualApplication": {
    "Path": "/Service.LegalParty"
  },
  "Security": {
    "Authority": "#LOCALSERVERNAME/common.security",
    "RequireHttpsMetadata": "false",
    "ApplicationName": "api.LegalPartyService"
  }
}
